#include <iostream>
using namespace std;
int main()
{
	int m, y;
	cin >> m >> y;
	if (y < 0 || (m < 1 || m>12)) {
		cout << "INVALID";

	}
	else if (y > 0) {
		if (m == 1 || m == 3 || m == 5 || m == 7 || m == 8 || m == 10 || m == 12) {
			cout << "31";
		}
		else if (m == 2) {
			if ((y % 4 == 0 && y % 100 != 0) || (y % 400 == 0))  {
				cout << "29";
			}
			else cout << "28";
		}
		else cout << "30";
	}
}
//Tìm số ngày của các tháng trong năm, bao gồm trường hợp năm nhuận
