#include <bits/stdc++.h>
using namespace std;

long long n, a[1000001], ma = -1e18;

int main() { 
    cin>>n; 
    for(int i=1;i<=n;i++) {
         cin>>a[i]; 
    }
   
sort (a+2, a + n);
for(int i=1;i<=n;i++){
    cout<<a[i]<<" ";
  }
} //Way 1
------------------------------------------------------------------
#include <bits/stdc++.h>
using namespace std;
int main ()
{
    int n;
    cin>>n;
    vector<int>a(n);
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    if(n>2){
        sort(a.begin()+1,a.end()-1);
    }
   for(int i=0;i<n;i++){
           cout<<a[i]<<" ";
       }

} //Way 2


//Sắp xếp mảng tăng dần, giữ lại thứ tự phần tử đầu và cuối
